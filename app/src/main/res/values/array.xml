<?xml version="1.0" encoding="utf-8"?>
<resources>

    <array name="indicator_colors">
        <item>@color/white</item>
        <!--Default-->
        <item>@color/violet</item>
        <!--Wrong-->
        <item>@color/navigationBarColorSkipped</item>
        <!--Right-->
        <item>@color/colorAccent</item>
        <!--Skipped-->
        <item>@color/colorAccent</item>
        <!--Current-->
        <item>@color/navigationBarColorAttempted</item>
        <!--Processed-->

    </array>
    <array name="border_state_colors">
        <item>@color/navigationBarColorAttempted</item>
        <!--Default-->
        <item>@color/violet</item>
        <!--Wrong-->
        <item>@color/navigationBarColorSkipped</item>
        <!--Right-->
        <item>@color/colorAccent</item>
        <!--Skipped-->
        <item>@color/colorAccent</item>
        <!--Current-->
        <item>@color/navigationBarColorAttempted</item>
        <!--Processed-->

    </array>

    <array name="text_state_colors">
        <item>@color/black</item>
        <!--Default-->
        <item>@color/violet</item>
        <!--Wrong-->
        <item>@color/lightGreen</item>
        <!--Right-->
        <item>@color/white</item>
        <!--Skipped-->
        <item>@color/white</item>
        <!--Current-->
        <item>@color/white</item>
        <!--Processed-->
    </array>

</resources>